{
  "name": "pollen-solutions/wp-tb",
  "description": "Wordpress TigreBlanc Component",
  "homepage": "https://www.presstify.com/pollen-solutions/wp-tb/",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "tigreblanc"
  ],
  "time": "2021-03-28",
  "license": "proprietary",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "pollen-solutions/support": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\WpTb\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\WpTb\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config" : {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}

